{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "autoscaling:Describe*"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Action": [
        "cloudformation:CreateStack",
        "cloudformation:DeleteStack",
        "cloudformation:DescribeStackEvents",
        "cloudformation:DescribeStackResource",
        "cloudformation:DescribeStackResources",
        "cloudformation:UpdateStack"
      ],
      "Effect": "Allow",
      "Resource": "arn:aws:cloudformation:*:*:stack/mustrace-frontend/*"
    },
    {
      "Action": [
        "cloudformation:DescribeStacks",
        "cloudformation:List*"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Sid": "Stmt1448297560000",
      "Effect": "Allow",
      "Action": [
        "s3:*"
      ],
      "Resource": [
        "arn:aws:s3:::beta.mustrace.com",
        "arn:aws:s3:::beta.mustrace.com/*",
        "arn:aws:s3:::www.beta.mustrace.com",
        "arn:aws:s3:::www.beta.mustrace.com/*"
      ]
    },
    {
      "Effect":"Allow",
      "Action":["route53:ListHostedZones"],
      "Resource":"*"
    },
    {
      "Effect":"Allow",
      "Action":["route53:ChangeResourceRecordSets"],
      "Resource":"arn:aws:route53:::hostedzone/Z2GMIABC1G5SUV"
    },
    {
      "Effect":"Allow",
      "Action":["route53:GetChange"],
      "Resource":"arn:aws:route53:::change/*"
    }
  ]
}
